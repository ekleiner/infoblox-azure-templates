{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string"
    },
    "virtualNetworkName": {
      "type": "string"
    },
    "virtualNetworkAddressPrefix": {
      "type": "string"
    },
    "subnet1Name": {
      "type": "string"
    },
    "subnet1Prefix": {
      "type": "string"
    },
    "subnet2Name": {
      "type": "string"
    },
    "subnet2Prefix": {
      "type": "string"
    },
    "vnetExistingRGName": {
      "type": "string",
      "defaultValue": ""
    }
  },
  "variables": {
    "vnetID": "[resourceId(parameters('vnetExistingRGName'),concat('Microsoft.Network','/','virtualNetworks'),parameters('virtualNetworkName'))]"
  },
  "resources": [],
  "outputs": {
    "subnet1Ref": {
      "value": "[concat(variables('vnetID'),'/subnets/',parameters('subnet1Name'))]",
      "type": "string"
    },
    "subnet2Ref": {
      "value": "[concat(variables('vnetID'),'/subnets/',parameters('subnet2Name'))]",
      "type": "string"
    }
  }
}
